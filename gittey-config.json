{
    "branchPrefix": {
        "separator": "",
        "validator": "",
        "prefixes": {}
    },
    "commitPrefix": {
        "separator": " ",
        "validator": "^.{1,45}$",
        "prefixes": {
            "feat:": "Feature work",
            "fix:": "Bug fix",
            "refactor:": "Refactoring",
            "test:": "Test",
            "chore:": "Chore",
            "docs:": "Documentation updated/added",
            "style:": "Code style",
            "build:": "Build",
            "ci:": "CI configuration",
            "perf:": "Performance improvement",
            "temp:": "Not building or passing tests but needs to be kept"
        }
    },
    "verboseMode": false,
    "aliases": [
        {
            "name": "test",
            "command": "cls; gittey commit"
        },
        {
            "name": "test-new",
            "command": "cls; dotnet run --project ./Test.Scripts/ && gittey commit"
        },
        {
            "name": "push",
            "command": "gittey test && git push"
        },
        {
            "name": "build",
            "command": "dotnet build"
        },
        {
            "name": "undo",
            "command": "git reset . && git checkout . && git clean -xfd"
        },
        {
            "name": "pack",
            "command": "gittey test && gittey up-version && gittey push && dotnet pack"
        },
        {
            "name": "pack-unsafe",
            "command": "gittey up-version && dotnet pack ./Lib/"
        },
        {
            "name": "up-version",
            "command": "gittey commit && \"DotVersion.exe\" --subpatch ./Lib/"
        }
    ],
    "collaborators": []
}